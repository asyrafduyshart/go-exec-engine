    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "name": "jojomama",
            "type": "string",
            "doc": "Test data schema"
        }
    ],
    "required": [
        "name",
        "type",
        "doc"
    ],
    "properties": {
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "jojomama"
            ]
        },
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "string"
            ]
        },
        "doc": {
            "$id": "#/properties/doc",
            "type": "string",
            "title": "The doc schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Test data schema"
            ]
        }
    },
    "additionalProperties": true
}